{
  "manifest_version": 3,
  "name": "De-Adlock",
  "key": "",
  "version": "1.0",
  "icons":{
    "16":"images/icon16.png",
    "32":"images/icon32.png",
    "48":"images/icon48.png",
    "128":"images/icon128.png"
  },
  "description":"The worlds first AI running Anti-Ad & Tracker. Blocks ads and trackers based on AI learned on more than 10K websites.",
  "declarative_net_request":{
    "rule_resources":[
      {
      "id": "ruleset_1",
      "enabled": true,
      "path":"rules.json"
      }
    ]
  },
  "host_permissions": [ "<all_urls>" ],
  "permissions": [
    "webRequest",
    "declarativeNetRequestWithHostAccess",
    "activeTab",
    "webNavigation",
    "declarativeNetRequestFeedback",
    "storage",
    "offscreen",
    "identity",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/icon.png"
  },
  "background": {
    "service_worker": "background.js",
    "type":"module"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; worker-src 'self' 'wasm-unsafe-eval'; object-src 'self' 'wasm-unsafe-eval';"
  }
}